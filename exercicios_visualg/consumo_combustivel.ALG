Algoritmo "consumo_combustivel"

Var

   kmTotal, combustivelUtilizado, media : real

Inicio

  escreva("Qual o km total percorido?")
  leia(kmtotal)
  escreva("Qual foi a quantidade de combustivél utilizada?")
  leia(combustivelutilizado)
  
  media <- kmTotal / combustivelUtilizado
  
  escreva("A média de consumo é de:", media:4:3, "Km/L")



Fimalgoritmo